{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "commonjs",
    "lib": [ "dom", "es6", "es2015.promise" ],
    "target": "es6",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "sourceMap": true
  },
  "include": [
      "src/",
  ],
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings",
    "dist"
  ]
}